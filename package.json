{
  "name": "cloudbees-casc-client-ui",
  "version": "1.0.0",
  "description": "CloudBees CasC API User Interface",
  "private": true,
  "scripts": {
    "dev": "webpack --config webpack.config.js --watch",
    "prod": "webpack --config webpack.config.js --mode=production",
    "start": "npm run dev --watch",
    "lint:js": "eslint src/main/js --ext js",
