    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.8.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "less": "^3.11.1",
    "less-loader": "^7.0.0",
    "mini-css-extract-plugin": "^0.11.0",
    "postcss": "^8.3.8",
    "postcss-less": "^3.1.4",
    "postcss-loader": "^4.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-remove-empty-scripts": "^0.7.1"
